{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "no-multiple-empty-lines": ["error", {"max": 2}],
        "no-extraneous-dependencies": "off"
    }
}
